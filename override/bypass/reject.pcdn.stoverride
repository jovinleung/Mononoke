name: pcdn 拒绝
desc: |-
    - https://www.v2ex.com/t/1127079
    - 屏蔽 CN STUN 出站
category: ruleset
icon: https://cdn.jsdelivr.net/gh/Koolson/Qure@master/IconSet/Color/Reject.png

rules:
    - AND,((PROTOCOL,STUN),(GEOIP,cn,no-resolve)),REJECT,no-track
    - DOMAIN,tp2p.kg.qq.com,REJECT,no-track
    - DOMAIN,twns.p2ptun.qq.com,REJECT,no-track
    - DOMAIN,sd-gl.xinqiucc.com,REJECT,no-track
    - DOMAIN,xyp-n.kuiniuca.com,REJECT,no-track
    - DOMAIN,sdk1xyajs.data.kuiniuca.com,REJECT,no-track
    - DOMAIN,ac.dun.163.com,REJECT,no-track
    - DOMAIN,fp-upload.dun.163.com,REJECT,no-track
    - DOMAIN-SUFFIX,v1d.szbdyd.com,REJECT,no-track
    - DOMAIN-SUFFIX,mcdn.bilivideo.cn,REJECT,no-track
    - RULE-SET,anti-AD.pcdn,REJECT,no-resolve,no-track
    - RULE-SET,PCDNFilter,REJECT,no-resolve,no-track
    - RULE-SET,Block-pcdn-domains,REJECT,no-resolve,no-track

rule-providers:
    anti-AD.pcdn:
        behavior: domain
        format: text
        interval: 86400
        type: http
        url: https://raw.githubusercontent.com/privacy-protection-tools/anti-AD/master/discretion/pcdn.txt

    # https://cdn.jsdelivr.net/gh/susetao/PCDNFilter-CHN-@main/PCDNFilter.txt
    PCDNFilter:
        behavior: domain
        format: yaml
        interval: 86400
        type: http
        url: https://p.19940731.xyz/api/stash/ruleset/adblock?url=https%3A%2F%2Fcdn.jsdelivr.net%2Fgh%2Fsusetao%2FPCDNFilter-CHN-%40main%2FPCDNFilter.txt

    # https://thhbdd.github.io/Block-pcdn-domains/ban.txt
    Block-pcdn-domains:
        behavior: domain
        format: text
        interval: 86400
        type: http
        url: https://p.19940731.xyz/api/stash/ruleset/adblock?url=https%3A%2F%2Fthhbdd.github.io%2FBlock-pcdn-domains%2Fban.txt
